<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.superstore">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SuperStore"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".activity_customer_page2"
            android:label="@string/title_activity_customer_page2"
            android:theme="@style/Theme.SuperStore.NoActionBar"></activity>
        <activity
            android:name=".activity_Employee_page2"
            android:label="@string/title_activity__employee_page2"
            android:theme="@style/Theme.SuperStore.NoActionBar" />
        <activity
            android:name=".activity_Employee"
            android:label="@string/title_activity__employee"
            android:theme="@style/Theme.SuperStore.NoActionBar" />
        <activity
            android:name=".activity_manager"
            android:label="@string/title_activity_manager"
            android:theme="@style/Theme.SuperStore.NoActionBar" />
        <activity
            android:name=".activity_customer"
            android:label="@string/title_activity_customer"
            android:theme="@style/Theme.SuperStore.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.SuperStore.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>